[unit]
Description = Eclipse fog05 Native Plugin



[Service]
Type=simple
PIDFile=/var/fos/hv-native.pid
ExecStart = /usr/bin/fos-hv-native -c /etc/fos/native-hypervisor/config.yaml
KillMode=mixed
KillSignal=SIGINT
RestartKillSignal=SIGINT
Restart=on-failure
User=root
[Install]
WantedBy=multi-user.target
